using System;

class Program
{
    static void func()
    {
        int V = 1;
        for (int i = 1; i < 6; i++)
        {
            for (int y = 1; y <= i; y++)
            {
                Console.Write("{0} ", V);
            }
            V++;
            Console.WriteLine();
            for (int y = 1; y <= i; y++)
            {
                Console.Write("{0} ", 0);
            }
            Console.WriteLine();
        }
    }
    static void Main(string[] args)
    {
        func();
    }
}

