using System;

namespace cZad_8
{
class Program
{
static bool f(string str, string num)
{
Regex regex = new Regex(@"([0-9]{1,3}[\.]){3}[0-9]{1,3}");
if (regex.IsMatch(str))
{
Console.WriteLine("Match");
string[] arr = str.Split('.');
Console.WriteLine(arr.Length);
if (arr[arr.Length-1][0].ToString()==num)
{
return true;
}
}
return false;
}
static void Main(string[] args)
{
try
{
string result = "";
Console.WriteLine("Введите строку, разделяя слова пробелами: ");
string input = Console.ReadLine();
Console.Write("Введите цифру: ");
int num = int.Parse(Console.ReadLine());
string[] masInput = input.Split(' ');
StringBuilder stringBuilder = new StringBuilder(input);
foreach (string s in masInput)
{
if (!f(s, num.ToString()))
{
result += s + ' ';
}
}
Console.WriteLine("Результат:\n{0}", result);
}
catch (Exception exc)
{

Console.WriteLine(exc.Message);
}
}
}
}
